{
  "name": "keytar",
  "description": "Bindings to native Mac/Linux/Windows password APIs",
  "version": "3.0.0",
  "licenses": [
    {
      "type": "MIT",
      "url": "http://github.com/atom/node-keytar/raw/master/LICENSE.md"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mongodb-js/node-keytar.git"
  },
  "bugs": {
    "url": "https://github.com/atom/node-keytar/issues"
  },
  "homepage": "http://atom.github.io/node-keytar",
  "keywords": [
    "keychain",
    "password",
    "passwords",
    "credential",
    "credentials",
    "vault",
    "credential vault"
  ],
  "scripts": {
    "fmt": "mongodb-js-fmt",
    "check-js": "mongodb-js-precommit",
    "check-cpp": "./node_modules/.bin/node-cpplint --verbose src/*.cc",
    "check": "npm run check-cpp && npm run check-js",
    "_install": "./node_modules/.bin/prebuild --download --verbose",
    "_rebuild": "./node_modules/.bin/prebuild --compile --verbose",
    "_prebuild": "./node_modules/.bin/prebuild --verbose",
    "test-node": "mocha",
    "test-main": "electron-mocha",
    "test-renderer": "electron-mocha --renderer",
    "test": "npm run test-node && npm run test-main && npm run test-renderer",
    "ci": "npm run check && npm test"
  },
  "devDependencies": {
    "electron-mocha": "^0.6.1",
    "electron-prebuilt": "^0.34.3",
    "eslint-config-mongodb-js": "^1.0.6",
    "mocha": "^2.3.4",
    "mongodb-js-fmt": "0.0.3",
    "mongodb-js-precommit": "^0.2.8",
    "node-cpplint": "~0.1.5",
    "rimraf": "~2.1.4"
  },
  "dependencies": {
    "nan": "^2.0.0",
    "prebuild": "^2.6.2"
  }
}
